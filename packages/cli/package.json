{
  "name": "@wasmer/cli",
  "description": "The CLI for executing Wasmer-JS",
  "version": "0.4.0",
  "author": "Wasmer Engineering Team <engineering@wasmer.io>",
  "license": "MIT",
  "bin": {
    "wasmer-js": "./bin/wasmer-js"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@wasmer/wasi": "^0.4.0",
    "@wasmer/wasm-transformer": "^0.4.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.0",
    "chai": "^4.2.0",
    "globby": "^10.0.1",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wasmer-js",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "wasmerio/wasmer-js",
  "bugs": {
    "url": "https://github.com/wasmerio/wasmer-js/issues"
  },
  "homepage": "https://github.com/wasmerio/wasmer-js/tree/master/packages/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "build": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepack": "npm run build",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "gitHead": "889c22135c81610ddecbb696f8ef24f7ecaf5db7"
}
