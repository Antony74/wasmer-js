{
  "version": "0.0.1",
  "commands": {
    "run": {
      "id": "run",
      "description": "Run a WebAssembly file with Wasmer-JS",
      "pluginName": "@wasmer/cli",
      "pluginType": "core",
      "aliases": [],
      "examples": ["$ wasmer-js run hello.wasm\nhello world\n"],
      "flags": {
        "dir": { "name": "dir", "type": "option" },
        "help": {
          "name": "help",
          "type": "boolean",
          "char": "h",
          "description": "show CLI help",
          "allowNo": false
        }
      },
      "args": [{ "name": "file" }]
    }
  }
}
