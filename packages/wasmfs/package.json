  "bugs": {
    "url": "https://github.com/wasmerio/wasmer-js/issues"
  },
  "homepage": "https://github.com/wasmerio/wasmer-js/tree/master/packages/wasmfs",
  "dependencies": {
    "memfs": "^2.15.5"
  }
}
